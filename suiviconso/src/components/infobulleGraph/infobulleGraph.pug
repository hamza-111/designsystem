mixin infobulleGraph(data)
	- data = data || {}
	- infoComparaison = data.infoComparaison || false 

	// .c-infobulleGraph
	if infoComparaison
		.c-infobulleGraph.u-shadow
			.c-infobulleGraph__header
				h3.u-textCenter Du 11 février au 10 mars 2022
			
			.c-infobulleGraph__lists
				ul.c-infobulleGraph__liste
					li
						span.c-infobulleGraph--puce.c-infobulleGraph--puceBlue Abonnement
						span.c-infobulleGraph--value <b>17,2€</b>
					li
						span.c-infobulleGraph--puce.c-infobulleGraph--puceRed Taxes
						span.c-infobulleGraph--value <b>66,2€</b>
					li
						span.c-infobulleGraph--puce.c-infobulleGraph--puceGreen Consommation
						span.c-infobulleGraph--value <b>232,2€</b>
				ul.c-infobulleGraph__liste.c-infobulleGraph__listeLeft
					li
						span.c-infobulleGraph--puceDefault Relevé réel du 13.12.2021
						span.c-infobulleGraph--value <b>3 712 m3</b>
					li
						span.c-infobulleGraph--puceDefault Conso gaz naturel
						span.c-infobulleGraph--value <b>2 696 kWh</b>
					li
						span.c-infobulleGraph--puceDefault Coefficient de conversion
						span.c-infobulleGraph--value <b>11,3 kWh/m3</b>

