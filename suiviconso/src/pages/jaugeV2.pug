//----------------------
//- DO NOT EDIT
extends pug-partials/base
//----------------------
include ../components/jauge/jauge

//- Main variables
block variables
  - var title           = "Jauge version 2"
  - var bodyClass       = "u-bg-grey-light"
  - var description     = ""
  - var noHeaderFooter  = true
  - var headerCel       = 'full'
  - var wrapperSite     = "l-wrapper--mainFull"
  - var footer          = true
  - var noBg            = true

//- JS only for that page
block JS


//- Page content
block content

  .l-row
    .l-col-large-8
      +jaugeV2
  
  .l-row.u-margin-t-40
    .l-col-large-8
      +jaugeV2Mens({
        img:"jauge-facture.jpg",
        imgMobile:"jauge-facture-mobile.jpg"
      })
  
  .l-row.u-margin-t-40.u-margin-b-40
    .l-col-large-8
      +jaugeV2Mens({
        img:"jauge-error.jpg",
        imgMobile:"jauge-error-mobile.jpg",
        error : true
      })