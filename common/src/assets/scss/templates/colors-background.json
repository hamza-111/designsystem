[
  <%= colors.map(function(color) {
    var obj = {};

    obj.title   = color +"-medium";
    obj.class   = "u-bg-"+ color +"-medium";
    obj.height  = "";
    obj.width   = "";
    obj.posX    = "";
    obj.posY    = "";

    return JSON.stringify(obj);
  }) %>,

  <%= colors.map(function(color) {
    var obj = {};

    obj.title   = color +"-light";
    obj.class   = "u-bg-"+ color +"-light";
    obj.height  = "";
    obj.width   = "";
    obj.posX    = "";
    obj.posY    = "";

    return JSON.stringify(obj);
  }) %>
]
