include fullModalHeader/fullModalHeader

mixin fullModal(data)
  - var data  = data || {}
  - var onlyTitle = data.onlyTitle || false

  // .c-fullModal
  .c-fullModal(aria-live="assertive")&attributes(attributes)
    +fullModalHeader({
      onlyTitle: onlyTitle
    })(data-e-sticky-header)

    .c-fullModal__main
      block

    footer.c-fullModal__footer
      +legalMention({
        settings: {
          target: "legalMention"
        }
      })
