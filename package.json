{
  "name": "reborn-tr",
  "version": "1.0.1",
  "description": "REBORN TR root",
  "private": true,
  "config": {
    "mode": "dev"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:common": "cd common && gulp prod --rebornVersion %npm_package_version%",
    "build:header": "cd header && gulp prod --rebornVersion %npm_package_version%",
    "build:cel": "cd cel && gulp prod --rebornVersion %npm_package_version%",
    "build:suiviconso": "cd suiviconso && gulp prod --rebornVersion %npm_package_version%",
    "build:headerCel": "cd headerCel && gulp prod --rebornVersion %npm_package_version%",
    "build:footer": "cd footer && gulp prod --rebornVersion %npm_package_version%",
    "build": "npm run build:common && npm run build:cel && npm run build:suiviconso && npm run build:header && npm run build:headerCel && npm run build:footer",
    "version": "",
    "postversion": "node changelog-update.js && git commit -a -m \"Update changelog\" && git push origin dev && git push origin --tags && git checkout main && git fetch origin && git reset --hard origin/main && git merge dev && git push origin main && npm run deploy:main && npm run bumpTagDist",
    "bumpTagDist": "cd .publish && git tag -a %npm_package_version% -m \"Bump version %npm_package_version%\" && git push --tags",
    "deploy:dev": "npm run build && gulp deploy:dev",
    "deploy:main": "npm run build && gulp deploy:main"
  },
  "main": "index.js",
  "author": "",
  "license": "private",
