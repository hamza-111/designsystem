    "gulp-imagemin": "3.3.0",
    "gulp-jshint": "2.0.4",
    "gulp-load-plugins": "1.4.0",
    "gulp-modernizr": "1.0.0-alpha",
    "gulp-nodemon": "2.2.1",
    "gulp-pug": "3.3.0",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.4",
